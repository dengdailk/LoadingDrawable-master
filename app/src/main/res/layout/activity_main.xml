<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="50dp"
    android:gravity="center">

    <Button
        android:id="@+id/shape_change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="31dp"
        android:text="ShapeChangeActivity"
        android:textAllCaps="false"
        app:layout_constraintBottom_toTopOf="@+id/goods"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/goods"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="30dp"
        android:text="GoodsActivity"
        android:textAllCaps="false"
        app:layout_constraintBottom_toTopOf="@+id/animal"
        app:layout_constraintStart_toStartOf="@+id/animal" />

    <Button
        android:id="@+id/animal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginBottom="30dp"
        android:text="AnimalActivity"
        android:textAllCaps="false"
        app:layout_constraintBottom_toTopOf="@+id/scenery"
        app:layout_constraintStart_toStartOf="@+id/scenery"
        tools:ignore="RtlHardcoded" />

    <Button
        android:id="@+id/scenery"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        android:layout_marginBottom="30dp"
        android:text="SceneryActivity"
        android:textAllCaps="false"
        app:layout_constraintBottom_toTopOf="@+id/circle_rotate"
        app:layout_constraintEnd_toEndOf="@+id/circle_rotate"
        tools:ignore="RtlHardcoded" />

    <Button
        android:id="@+id/circle_rotate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:text="CircleRotateActivity"
        android:textAllCaps="false"
        app:layout_constraintBottom_toTopOf="@+id/circle_jump"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/circle_jump"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginBottom="122dp"
        android:text="CircleJumpActivity"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/circle_rotate"
        tools:ignore="RtlHardcoded" />

</android.support.constraint.ConstraintLayout>
